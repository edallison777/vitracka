{
    "name": "vitracka-mobile",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.19.3",
        "@react-native-community/netinfo": "^9.5.0",
        "@react-navigation/bottom-tabs": "^6.5.8",
        "@react-navigation/native": "^6.1.7",
        "@react-navigation/stack": "^6.3.17",
        "@reduxjs/toolkit": "^1.9.5",
        "i18next": "^23.7.6",
        "react": "18.2.0",
        "react-i18next": "^13.5.0",
        "react-native": "0.72.4",
        "react-native-chart-kit": "^6.12.0",
        "react-native-device-info": "^10.11.0",
        "react-native-gesture-handler": "^2.12.1",
        "react-native-localize": "^3.0.3",
        "react-native-reanimated": "^3.5.4",
        "react-native-safe-area-context": "^4.7.2",
        "react-native-screens": "^3.25.0",
        "react-native-svg": "^13.4.0",
        "react-native-vector-icons": "^10.0.0",
        "react-redux": "^8.1.2",
        "redux-persist": "^6.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@react-native/eslint-config": "^0.72.2",
        "@react-native/metro-config": "^0.72.11",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react-native": "^12.4.2",
        "@tsconfig/react-native": "^3.0.0",
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.24",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.2.1",
        "babel-plugin-module-resolver": "^5.0.2",
        "eslint": "^8.19.0",
        "fast-check": "^3.15.0",
        "jest": "^29.2.1",
        "metro-react-native-babel-preset": "0.76.8",
        "prettier": "^2.4.1",
        "react-test-renderer": "18.2.0",
        "typescript": "4.8.4"
    },
    "jest": {
        "preset": "react-native",
        "setupFilesAfterEnv": [
            "@testing-library/jest-native/extend-expect",
            "<rootDir>/src/__tests__/setup/testSetup.ts"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!(react-native|@react-native|@react-native-community|@react-native-async-storage|react-native-vector-icons|react-native-chart-kit|react-native-svg|react-native-reanimated|react-native-gesture-handler|react-native-safe-area-context|react-native-screens|react-native-localize|react-native-device-info)/)"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/src/__tests__/setup/",
            "<rootDir>/src/__tests__/__mocks__/"
        ],
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/**/*.d.ts",
            "!src/__tests__/**",
            "!src/types/**"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
        },
        "testEnvironment": "node",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}